package classe2;

public class Funcionario {
	String nome;
//	double salario;
	String funcao;
	
	// Construtor
	Funcionario(String n, String f) {
		nome = n;
//		salario = s;
		funcao = f;
	}

	// Método
	
	String formatarFuncionario() {
		return String.format("%f/%f/%f", nome, salario, funcao);
	}
	
	
}
