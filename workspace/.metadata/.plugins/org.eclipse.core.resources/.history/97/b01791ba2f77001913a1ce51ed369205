package controleExercicios;

import java.util.Scanner;

public class Ex05 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int n = 0,contadorDeDivisores = 0;
		
		System.out.println("Digite um número: ");
		n = scanner.nextInt();
		
		for (int i =2;i < n;i++) {
			if (n % i == 0) {
				contadorDeDivisores++;
			}
		}
		
		
	}

}
