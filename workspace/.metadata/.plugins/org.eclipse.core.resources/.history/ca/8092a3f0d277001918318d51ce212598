		Scanner scanner = new Scanner(System.in);
		String l;		
				
		System.out.printf("Digite uma palavra: ");
		l = scanner.nextLine();
		char ch; 
		
		for (int i =0; l.length()>i; i++) {
			
			
			System.out.println(l.charAt(i););
					
			
		}
		
	}

}
