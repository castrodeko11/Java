package fundamentosExercicios;

import java.util.Scanner;
/*
 * 
 */

public class Ex2 {
	public static void main(String[] args) {

		float fht = (float) 0;
		Scanner ler = new Scanner(System.in);

		System.out.printf("Informe a temperatura em fahrenheit para conversão: ");
		fht = ler.nextFloat();

		System.out.println("Conversão da temperatura celsius: " + (5 / 9 * (fht - 32)) + " ºC");
		
		
		ler.close();

	}
}
